	},
	"devDependencies": {
		"@pucelle/webpack-svg-loader": "^1.0.0",
		"ts-patch": "^3.2.1",
		"typescript": "^5.6.2",
		"webpack": "^5.95.0",
		"webpack-dev-server": "^5.1.0"
	}
}
