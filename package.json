{
	"name": "ff-uix",
	"version": "0.1.2",
	"description": "A light weight Web UI & UX Framework, based on lupos",
	"author": "pucelle",
	"license": "MIT",
	"homepage": "https://github.com/pucelle/ff-uix",
	"repository": {
		"type": "git",
		"url": "https://github.com/pucelle/ff-uix.git"
	},
	"type": "module",
	"main": "out/index.js",
	"types": "out/index.d.ts",
	"sideEffects": false,
	"scripts": {
		"build": "luc -e",
		"watch": "luc -e -w",
		"build-docs": "cd docs && luc",
		"watch-docs": "cd docs && luc -w",
		"dev-docs": "webpack serve",
		"dev-bundled-docs": "webpack serve --mode=\"production\"",
		"bundle-docs": "webpack --mode=\"production\"",
		"prepublish": "npm run build && npm run build-docs"
	},
	"dependencies": {
		"ff-kit": "^2.0.1",
		"lupos": "^0.1.0",
		"lupos.html": "^0.1.1"
	},
	"devDependencies": {
		"@pucelle/webpack-svg-loader": "^1.0.0",
		"typescript": "^5.6.2",
		"webpack": "^5.98.0",
		"webpack-dev-server": "^5.2.1"
	}
}