{
	"name": "@pucelle/ff",
	"version": "1.0.0",
	"description": "A light weight Web UI Framework, based on lupos.js",
	"author": "pucelle",
	"license": "MIT",
	"homepage": "https://github.com/pucelle/lupos.ui",
	"repository": {
		"type": "git",
		"url": "https://github.com/pucelle/lupos.ui.git"
	},
	"main": "out/index.js",
	"types": "out/index.d.ts",
	"sideEffects": false,
	"scripts": {
